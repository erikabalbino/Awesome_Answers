<ul>
  <% @questions.each do |question| %>
    <li>
      <%= link_to(
        question.title,
        question_path(question.id),
        class: "awesome-tolltip",
        title: " 👀 #{question.view_count} ❤️ #{question.likes.count}"
      ) %>
      <br />
      <small>
        Posted <%= time_ago_in_words(question.created_at) %> ago
      </small>
    </li>
  <% end %>
</ul>

